{
  "name": "crass-loader",
  "version": "1.0.3",
  "description": "Crass loader module for webpack",
  "engines" : { "npm" : ">5.2.0" },
  "keywords": [
    "webpack",
    "loader",
    "crass",
    "css",
    "minify"
  ],
  "scripts": {
    "precommit": "lint-staged",
